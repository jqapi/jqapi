{
  "name": "jqapi",
  "version": "2.0.0",
  "author": "Sebastian Senf <contact@mustardamus.com>",
  "description": "Alternative jQuery Documentation Browser",
  "keywords": [
    "jquery",
    "documentation",
    "browser"
  ],
  "license": "MIT",
  "homepage": "https://jqapi.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jqapi/jqapi.git"
  },
  "bugs": {
    "url": "https://github.com/jqapi/jqapi/issues"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test:watch": "vitest watch",
    "test:update": "vitest --update",
    "test": "vitest",
    "docs:update": "git submodule update --remote",
    "docs:generate": "node lib/categories-xml-to-json.mjs",
    "docs:copy": "cp -RT api.jquery.com/resources public/resources",
    "docs": "run-s docs:*"
  },
  "dependencies": {
    "astro": "^1.9.2",
    "astro-purgecss": "^1.3.2",
    "bulma": "^0.9.4",
    "jquery": "^3.6.3",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.3",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.57.1",
    "vitest": "^0.27.0"
  }
}
